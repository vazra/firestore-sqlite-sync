{
  "name": "@firestore-sqlite-sync/core",
  "version": "0.1.0",
  "description": "Sync selected collections from firebase-firestore to local sqlite database",
  "type": "commonjs",
  "main": "./dist/sample/src/index.js",
  "types": "./dist/sample/src/index.d.ts",
  "scripts": {
    "start": "webpack --mode=development --watch --progress",
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "shx rm -rf dist/*",
    "buildold": "tsc",
    "watch": "tsc --watch",
    "test": "mocha --ui qunit",
    "testall": "mocha --ui qunit dist/sample/test",
    "prepack": "npm run clean && npm run build"
  },
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "knex": "^0.21.1",
    "faker": "4.1.0",
    "lodash": "^4.17.15",
    "firebase": "7.x"
  },
  "peerDependencies": {
    "chai": "1.x",
    "sqlite3": "4.x"
  },
  "// devDependencies": {
    "@types/node": "Needed for unit test assertions (assert.equal() etc.)",
    "shx": "Needed for development-time package.json scripts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.157",
    "@types/faker": "^4.1.12",
    "@types/firebase": "^3.2.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "12.x",
    "mocha": "^8.0.1",
    "shx": "^0.3.2",
    "typescript": "^3.9.5",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "@babel/core": "^7.10.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vazra/firestore-sqlite-sync.git"
  },
